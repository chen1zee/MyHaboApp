    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.19",
    "@types/react-test-renderer": "^16.9.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.8.6",
    "typescript": "^3.6.3"
  },
  "jest": {
    "reset": "react-native"
  }
